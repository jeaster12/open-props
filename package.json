{
  "type": "module",
  "name": "@cupcake-ds/cc-props",
  "license": "MIT",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeaster12/open-props"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "css",
    "utilities",
    "variables",
    "custom properties",
    "gradients"
  ],
  "style": "cc-props.min.css",
  "source": "src/index.js",
  "main": "dist/cc-props.cjs",
  "unpkg": "cc-props.min.css",
  "module": "dist/cc-props.module.js",
  "exports": {
    ".": {
      "import": "./dist/cc-props.module.js",
      "require": "./dist/cc-props.cjs",
      "default": "./dist/cc-props.cjs"
    },
    "./style": "./cc-props.min.css",
    "./postcss/style": "./src/index.css",
    "./borders": "./borders.min.css",
    "./colors": "./colors.min.css",
    "./colors-brand": "./colors.brand.min.css",
    "./global": "./global.min.css",
    "./fonts": "./fonts.min.css",
    "./media": "./media.min.css",
    "./shadows": "./shadows.min.css",
    "./sizes": "./sizes.min.css",
    "./zindex": "./zindex.min.css",
    "./json": "./cc-props.tokens.json",
    "./tokens": "./cc-props.tokens.json",
    "./design-tokens": "./cc-props.tokens.json"
  },
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "build": "npm run gen:op",
    "test": "ava test/basic.test.cjs",
    "pre-deploy": "npm run build && npm run bundle",
    "bundle": "concurrently npm:lib:*",
    "bundle:pre-edit": "json -I -f package.json -e \"this.unpkg=''\"",
    "bundle:post-edit": "json -I -f package.json -e \"this.unpkg='cc-props.min.css'\"",
    "gen:op": "cd src && node _create-props.js '' true",
    "gen:nowhere": "cd src && node _create-props.js '' false",
    "gen:prefixed": "cd src && node _create-props.js 'op'",
    "lib:js": "npm run bundle:pre-edit && microbundle --no-sourcemap && npm run bundle:post-edit",
    "lib:all": "postcss src/index.css -o cc-props.min.css",
    "lib:normalize": "postcss src/extra/normalize.css -o normalize.min.css",
    "lib:borders": "postcss src/props.borders.css -o borders.min.css",
    "lib:global": "postcss src/props.global.css -o global.min.css",
    "lib:fonts": "postcss src/props.fonts.css -o fonts.min.css",
    "lib:media": "postcss src/props.media.css -o media.min.css",
    "lib:shadows": "postcss src/props.shadows.css -o shadows.min.css",
    "lib:sizes": "postcss src/props.sizes.css -o sizes.min.css",
    "lib:supports": "postcss src/props.supports.css -o supports.min.css",
    "lib:zindex": "postcss src/props.zindex.css -o zindex.min.css",
    "lib:colors": "postcss src/props.colors.css -o colors.min.css",
    "lib:colors:brand": "postcss src/props.colors.brand.css -o colors-brand.min.css"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "concurrently": "^6.2.1",
    "cssnano": "^5.0.8",
    "json": "^11.0.0",
    "microbundle": "^0.13.3",
    "open-color": "^1.9.1",
    "postcss": "^8.3.9",
    "postcss-cli": "^8.3.1",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "6.7.x"
  }
}
